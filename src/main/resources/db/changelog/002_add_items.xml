<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="2" author="rpovetkin">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">SELECT COUNT(*) FROM item</sqlCheck>
        </preConditions>
        <insert tableName="item">
            <column name="title" value="1title"/>
            <column name="description" value="1description"/>
            <column name="img_path" value="test/path/1.jpg"/>
            <column name="count" valueNumeric="1"/>
            <column name="price" valueNumeric="10.00"/>
        </insert>
        <insert tableName="item">
            <column name="title" value="2title"/>
            <column name="description" value="2description"/>
            <column name="img_path" value="test/path/2.jpg"/>
            <column name="count" valueNumeric="2"/>
            <column name="price" valueNumeric="2.00"/>
        </insert>
    </changeSet>
</databaseChangeLog>